.header
	display flex
	align-items center
	padding 50px
	justify-content space-between
	position fixed
	width 100%
	background linear-gradient(to bottom, rgba(0,0,0,0.31) 0%,rgba(0,0,0,0.13) 89%,rgba(0,0,0,0) 100%)
	&__logo
		img
			height 65px
	&__menu-list
		list-style-type none
		display flex
	&__menu-item
		margin-left 6px 
		margin-right 6px
		
	
	